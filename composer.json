{
    "name": "flowd/scheduler-forker",
    "type": "typo3-cms-extension",
    "description": "Run each TYPO3 scheduler task in a forked process",
    "homepage": "https://flowd.de",
    "license": ["GPL-2.0-or-later"],
    "authors": [{
        "name": "Sascha Egerer",
        "email": "sascha.egerer@flowd.de",
        "role": "Developer",
        "homepage": "https://flowd.de"
    }],
    "require": {
        "php": ">=7.1.0",
        "typo3/cms-core": "^8.7 || ^9.5",
        "typo3/cms-scheduler": "^8.7 || ^9.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "roave/security-advisories": "dev-master",
        "typo3/testing-framework": "^4.11",
        "phpstan/phpstan-strict-rules": "^0.10",
        "saschaegerer/phpstan-typo3": "0.10.x-dev"
    },
    "replace": {
        "scheduler_forker": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "scheduler_forker"
        }
    },
    "autoload": {
        "psr-4": {
            "Flowd\\SchedulerForker\\": "Classes/"
        }
    }
}
